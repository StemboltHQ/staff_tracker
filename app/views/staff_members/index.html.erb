<h1>Stembolt Staff</h1>
<%= link_to 'Add new staff member', new_staff_member_path %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <%= @staff_members.each do |staff_member| %>
    <tr>
      <td><%= staff_member.first_name %></td>
      <td><%= link_to 'Show', staff_member_path(staff_member) %></td>
      <td><%= link_to 'Edit', edit_staff_member_path(staff_member) %></td>
    </tr>
    <% end %>
  </tbody>
</table>
